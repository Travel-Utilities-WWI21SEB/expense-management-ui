{
	"name": "expense-management-ui",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": "^19.9.0",
		"pnpm": "^8.6.0"
	},
	"packageManager": "pnpm@8.6.4",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prep-commit": "pnpm check && pnpm format",
		"prep-pr": "pnpm check && pnpm lint && pnpm build",
		"docker:build": "docker build -t expense-ui .",
		"docker:run": "docker run -d -p 4174:3000 --name expense-ui-container expense-ui",
		"docker:stop": "docker stop expense-ui-container && docker rm expense-ui-container"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "1.8.0",
		"@sveltejs/adapter-node": "1.2.4",
		"@sveltejs/kit": "1.20.5",
		"@tailwindcss/forms": "0.5.3",
		"@types/lodash": "^4.14.195",
		"@types/node": "20.2.5",
		"@typescript-eslint/eslint-plugin": "5.60.0",
		"@typescript-eslint/parser": "5.60.0",
		"autoprefixer": "10.4.14",
		"eslint": "8.43.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.31.1",
		"postcss": "8.4.24",
		"postcss-load-config": "4.0.1",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "3.59.2",
		"svelte-check": "3.4.4",
		"svelte-preprocess": "5.0.4",
		"tailwindcss": "3.3.2",
		"tslib": "2.5.3",
		"typescript": "5.1.3",
		"vite": "4.3.9"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.3.0",
		"chart.js": "4.3.0",
		"dotenv": "^16.1.4",
		"jwt-decode": "^3.1.2",
		"lodash": "4.17.21",
		"svelte-chartjs": "3.1.2"
	}
}
