{
	"name": "expense-management-ui",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"node": "^20.4.0",
		"pnpm": "^8.6.0"
	},
	"packageManager": "pnpm@8.6.7",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prep-commit": "pnpm check && pnpm format",
		"prep-pr": "pnpm check && pnpm lint && pnpm build",
		"docker:build": "docker build -t expense-ui .",
		"docker:run": "docker run -d -p 4174:3000 --name expense-ui-container expense-ui",
		"docker:stop": "docker stop expense-ui-container && docker rm expense-ui-container"
	},
	"devDependencies": {
		"@inlang/sdk-js": "^0.8.1",
		"@skeletonlabs/skeleton": "1.9.0",
		"@steeze-ui/heroicons": "^2.2.3",
		"@sveltejs/adapter-node": "1.3.1",
		"@sveltejs/kit": "1.22.3",
		"@tailwindcss/forms": "0.5.4",
		"@types/lodash": "^4.14.195",
		"@types/node": "20.4.2",
		"@typescript-eslint/eslint-plugin": "5.61.0",
		"@typescript-eslint/parser": "5.60.1",
		"autoprefixer": "10.4.14",
		"eslint": "8.44.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.32.2",
		"postcss": "8.4.26",
		"postcss-load-config": "4.0.1",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "3.59.2",
		"svelte-carousel": "^1.0.25",
		"svelte-check": "3.4.6",
		"svelte-flag-icons": "^0.5.5",
		"svelte-swipe": "^1.9.2",
		"tailwindcss": "3.3.3",
		"tslib": "2.6.0",
		"typescript": "5.1.6",
		"vite": "4.4.3"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.4.4",
		"@steeze-ui/svelte-icon": "1.5.0",
		"chart.js": "4.3.0",
		"dotenv": "^16.3.1",
		"jwt-decode": "^3.1.2",
		"lodash": "4.17.21",
		"svelte-chartjs": "3.1.2"
	}
}
